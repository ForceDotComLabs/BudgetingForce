    <validationRules>
        <fullName>Budget_Line_Cannot_be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(ISNEW(), ISCHANGED( Budget_Line_Amount__c )) ,Budget_Line_Amount__c &lt; 0,  NOT(OR( $User.Username = &apos;mchan@budget.com&apos;, $User.Username = &apos;mhristova@budget.com&apos;, $User.Username = &apos;epark@budget.com&apos;,$User.Username = &apos;mrosenbaum@budget.com&apos;, $User.Username = &apos;dchang@budget.com&apos;, $User.Username = &apos;skeane@budget.com&apos;, $User.Username = &apos;jbadanguio@budget.com&apos;, $User.Username=&apos;aalvarez@budget.com&apos;, $User.Username=&apos;dfebles@budget.com&apos;)))</errorConditionFormula>
        <errorDisplayField>Budget_Line_Amount__c</errorDisplayField>
        <errorMessage>You are not allowed to enter a negative amount.  Please contact Marlene Chan for assistance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Budget_Line_Amount__c &lt;0,NOT(OR(ISPICKVAL(Budget_Type__c,&quot;MDF&quot;),ISPICKVAL(Budget_Type__c,&quot;Contingency&quot;))
)
)</errorConditionFormula>
        <errorDisplayField>Budget_Type__c</errorDisplayField>
        <errorMessage>Budget Type is required when Budget Line Amount is &lt; $0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_line_cannot_be_lower_than_prs</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISNEW(),ISCHANGED(Budget_Line_Amount__c)), Budget_Line_Amount__c&gt;=0,Budget_Line_Amount__c &lt;  Rollup_of_Purchase_Request_Amount__c)</errorConditionFormula>
        <errorDisplayField>Budget_Line_Amount__c</errorDisplayField>
        <errorMessage>The budget line cannot be less than the sum of the purchase request amounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Original_Budget_Amount</fullName>
        <active>false</active>
        <errorConditionFormula>OR( NOT(PRIORVALUE(Original_Budget_Line_Amount__c ) &lt;&gt; 0), NOT(ISNULL(Original_Budget_Line_Amount__c )))</errorConditionFormula>
        <errorMessage>You cannot update the Original Budget Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reject_Status</fullName>
        <active>true</active>
        <errorConditionFormula>text(Status__c)  = &quot;Rejected&quot;  &amp;&amp;   Reject_Reason__c  = &quot;&quot;</errorConditionFormula>
        <errorMessage>You must enter a Rejection Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISPICKVAL(PRIORVALUE(Status__c),&quot;Approved&quot;)), $Profile.Name &lt;&gt;&quot;System Administrator&quot;, ISPICKVAL(Status__c,&quot;Approved&quot;))</errorConditionFormula>
        <errorMessage>You are not allowed to update the Status to Approved.  Please submit an Approval Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Budget_cannot_exceed_Program</fullName>
        <active>true</active>
        <errorConditionFormula>OR((Program_Budget__r.Rollup_of_Unapproved_Budget_Lines__c  - PRIORVALUE(
Budget_Line_Amount__c ) + Budget_Line_Amount__c  &gt;  Program_Budget__r.Program_Budget_Amount__c), AND(ISNEW(), Program_Budget__r.Rollup_of_Unapproved_Budget_Lines__c  + Budget_Line_Amount__c  &gt;  Program_Budget__r.Program_Budget_Amount__c))</errorConditionFormula>
        <errorDisplayField>Budget_Line_Amount__c</errorDisplayField>
        <errorMessage>The Total sum of all budget line exceeds the Program Budget. You cannot make this change to the Budget Line. Please reduce another Budget Line before increasing this program.</errorMessage>
    </validationRules>
