    <validationRules>
        <fullName>Change_Amount_not_negative</fullName>
        <active>true</active>
        <errorConditionFormula>Change_Request_Amount__c&lt;0</errorConditionFormula>
        <errorDisplayField>Change_Request_Amount__c</errorDisplayField>
        <errorMessage>Please enter your change in absolute value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Request_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL( Request_Action__c  ,&quot;Change Request - Increase&quot;), ISPICKVAL(Request_Action__c ,&quot;Change Request - Decrease&quot;)) , 
AND(
NOT(OR(ISPICKVAL(Status__c,&quot;Change Request&quot;),ISPICKVAL(Status__c,&quot;In Progress&quot;),CONTAINS(TEXT(Status__c),&quot;Approved&quot;))))
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status must = Change Request, In Progress or Approved if Request Action = Increase or Decrease</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contractor_Change_Amount_not_negative</fullName>
        <active>true</active>
        <errorConditionFormula>Contractor_Change_Request_Amount__c &lt;0</errorConditionFormula>
        <errorDisplayField>Contractor_Change_Request_Amount__c</errorDisplayField>
        <errorMessage>Please enter your change in absolute value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PR_Action_Change_Amount_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(Request_Action__c, &quot;Change Request - Increase&quot;),ISPICKVAL(Request_Action__c, &quot;Change Request - Decrease&quot;)), OR(ISBLANK(Change_Request_Amount__c),ISNULL(Change_Request_Amount__c),Change_Request_Amount__c=0),
 $RecordType.Name = &quot;Program Spend&quot;
)</errorConditionFormula>
        <errorDisplayField>Change_Request_Amount__c</errorDisplayField>
        <errorMessage>Change Amount is required if the Request Action on the  Purchase Request = Increase or Decrease</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PR_Action_ContractChange_Amount_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(Request_Action__c, &quot;Increase&quot;),ISPICKVAL(Request_Action__c, &quot;Decrease&quot;)), OR(ISBLANK( Contractor_Change_Request_Amount__c ),ISNULL(Contractor_Change_Request_Amount__c ),Contractor_Change_Request_Amount__c =0),
 $RecordType.Name = &quot;Contractor Spend&quot;
)</errorConditionFormula>
        <errorDisplayField>Contractor_Change_Request_Amount__c</errorDisplayField>
        <errorMessage>Change Amount is required if the Request Action on the  Purchase Request = Increase or Decrease</errorMessage>
    </validationRules>
