    <validationRules>
        <fullName>Purchase_Request_cannot_exceed_Budget</fullName>
        <active>true</active>
        <errorConditionFormula>OR((Budget_Line__r.Rollup_of_Purchase_Request_Amount__c - PRIORVALUE( Total_PR_Amount_with_Changes__c ) + Total_PR_Amount_with_Changes__c &gt; Budget_Line__r.Budget_Line_Amount__c),AND(ISNEW(), Budget_Line__r.Rollup_of_Purchase_Request_Amount__c + Purchase_Request_Amount__c &gt; Budget_Line__r.Budget_Line_Amount__c)
)</errorConditionFormula>
        <errorDisplayField>Purchase_Request_Amount__c</errorDisplayField>
        <errorMessage>The sum of all the purchase requests for this budget line cannot exceed the approved budget amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_increase_decrease_required</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(OR( ISPICKVAL(Request_Action__c,&quot;Change Request - Increase&quot;), ISPICKVAL(Request_Action__c,&quot;Change Request - Decrease&quot;)),

OR(ISNULL(Reason_for_Increase_Decrease__c) , Reason_for_Increase_Decrease__c=&quot; &quot;,ISBLANK(Reason_for_Increase_Decrease__c)))
,
AND(OR(ISCHANGED(Request_Action__c),ISCHANGED( Change_Request_Amount__c ),ISCHANGED( Contractor_Change_Request_Amount__c)),  OR( ISPICKVAL(Request_Action__c,&quot;Change Request - Increase&quot;), ISPICKVAL(Request_Action__c,&quot;Change Request - Decrease&quot;)), 
NOT(ISCHANGED(Reason_for_Increase_Decrease__c)))
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Increase_Decrease__c</errorDisplayField>
        <errorMessage>You must enter a new reason for increase/decrease if this is a new request, or update the reason if you have made a change to the requested amount.</errorMessage>
    </validationRules>
