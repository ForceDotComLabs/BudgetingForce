NOT(OR( $User.Username = &apos;mchan@budget.com&apos;, $User.Username = &apos;mhristova@budget.com&apos;, $User.Username = &apos;epark@budget.com&apos;,$User.Username = &apos;mrosenbaum@budget.com&apos;, $User.Username = &apos;dchang@budget.com&apos;,  $User.Username = &apos;jbadanguio@budget.com&apos;, $User.Username = &apos;mchan@dreamforce.com&apos;, $User.Username = &apos;mhristova@dreamforce.com&apos;, $User.Username = &apos;epark@dreamforce.com&apos;,$User.Username = &apos;mrosenbaum@dreamforce.com&apos;, $User.Username = &apos;dchang@dreamforce.com&apos;,  $User.Username = &apos;jbadanguio@dreamforce.com&apos;)))</errorConditionFormula>
